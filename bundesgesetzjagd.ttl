    sl:parent :bundesgeseu3 ;
    sl:prev :bundesgeseu3 ;
    sl:title "Zweck"@de .

:bundesgeseu5 a sl:Absatz ;
    sl:content "Dieses Gesetz bezweckt:"@de ;
    sl:ord 1 ;
    sl:parent :bundesgeseu4 ;
    sl:prev :bundesgeseu4 .

