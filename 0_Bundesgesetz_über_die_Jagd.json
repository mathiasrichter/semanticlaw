            "parent": "bundesgesetzberdiejagdpeu",
            "prev": "bundesgesetzberdiejagdpeu",
            "next": "bundesgesetzberdiejagdpew",
            "ord": "a",
            "title": null,
            "content": "a.  Marderhund, Waschb\u00e4r und verwilderte Hauskatze; \n"
        },
        {
            "id": "bundesgesetzberdiejagdpew",
            "line_no": 190,
            "type": "Litera",
            "parent": "bundesgesetzberdiejagdpeu",
            "prev": "bundesgesetzberdiejagdpev",
            "next": "bundesgesetzberdiejagdpex",
